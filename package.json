    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.0",
    "babel-jest": "^29.5.0",
    "jest": "^29.5.0",
    "jest-css-modules": "^2.1.0"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "<rootDir>/.next/",
